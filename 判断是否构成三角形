#include<stdio.h>
int main()
{
    int n,i,a,b,c,max;
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        scanf("%d %d %d",&a,&b,&c);
        if(a>b)
            max=a;
        else
            max=b;
        if(c>max)
            max=c;
            if(max==a){(b+c>a)?printf("YES\n"):printf("NO\n");}
            else if(max==b){(a+c>b)?printf("YES\n"):printf("NO\n");}
            else if(max==c){(a+b>c)?printf("YES\n"):printf("NO\n");}
        }
}
