#include<stdio.h>
int main()
{
    int n,i,j=1;
    scanf("%d",&n);
    printf("Sun Mon Tue Wen Thu Fri Sat\n");

    if(n==7)
    {
          for(i=1;i<=30;i++)
          {
             if(j>7)
                {
                    j=1;
                    printf("\n");
                }
             if(j==1)
                printf("%3d",i);
             else
                printf(" %3d",i);
             j++;
          }
    }
    else
    {
        int m=n+1;
        if(n==1){
                    printf("   ");
                    for(i=1;i<=30;i++)
          {
             if(m>7)
                {
                    m=1;
                    printf("\n");
                }
             if(m==1)
                printf("%3d",i);
             else
                printf(" %3d",i);
             m++;
          }

                    }
        else if(n==2){
                      printf("       ");
                    for(i=1;i<=30;i++)
          {
             if(m>7)
                {
                    m=1;
                    printf("\n");
                }
            if(m==1)
                printf("%3d",i);
             else
                printf(" %3d",i);
             m++;
          }

                   }
        else if(n==3){
                        printf("           ");
                    for(i=1;i<=30;i++)
          {
             if(m>7)
                {
                    m=1;
                    printf("\n");
                }
             if(m==1)
                printf("%3d",i);
             else
                printf(" %3d",i);
             m++;
          }

                   }
        else if(n==4){
                        printf("               ");
                    for(i=1;i<=30;i++)
          {
             if(m>7)
                {
                    m=1;
                    printf("\n");
                }
            if(m==1)
                printf("%3d",i);
             else
                printf(" %3d",i);
             m++;
          }

                   }
        else if(n==5){
                        printf("                   ");
                    for(i=1;i<=30;i++)
          {
             if(m>7)
                {
                    m=1;
                    printf("\n");
                }
             if(m==1)
                printf("%3d",i);
             else
                printf(" %3d",i);
             m++;
          }

                   }
        else if(n==6){
                        printf("                       ");
                    for(i=1;i<=30;i++)
          {
             if(m>7)
                {
                    m=1;
                    printf("\n");
                }
             if(m==1)
                printf("%3d",i);
             else
                printf(" %3d",i);
             m++;
          }

                   }
    }
}
